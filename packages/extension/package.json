{
  "name": "time-trace-local",
  "displayName": "Time Trace Local",
  "publisher": "anticultist",
  "private": true,
  "description": "",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE.md",
  "pricing": "Free",
  "engines": {
    "vscode": "^1.102.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anticultist/time-trace-local"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "time tracking",
    "productivity",
    "local"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "timeTraceLocal.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "timeTraceLocal.configure",
        "title": "Configure",
        "icon": "$(gear)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "timeTraceLocal.refresh",
          "when": "view == timeTraceLocalDefaultView",
          "group": "navigation"
        },
        {
          "command": "timeTraceLocal.configure",
          "when": "view == timeTraceLocalDefaultView",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "time-trace-local",
          "title": "Time Trace Local",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "time-trace-local": [
        {
          "id": "timeTraceLocalDefaultView",
          "type": "webview",
          "name": "Time Trace Local",
          "icon": "media/logo.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "build": "pnpm run check-types && pnpm run lint && vite build",
    "watch": "npm-run-all -p watch:*",
    "watch:vite": "vite build --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && cross-env NODE_ENV=production vite build",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run build && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "clean": "rimraf --glob dist tsconfig.tsbuildinfo \"time-trace-local-*.vsix\""
  },
  "vsce": {
    "dependencies": false
  },
  "dependencies": {
    "@time-trace-local/services": "workspace:*",
    "@vscode/codicons": "^0.0.39"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.17.2",
    "@types/vscode": "^1.102.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "cross-env": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-static-copy": "^3.1.2"
  }
}
